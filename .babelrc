{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime"],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types", "@loadable/babel-plugin"]
    }
  }
}
